{
    "services": [
        {
            "name": "nginxhello",
            "backends": [
                {
                   "type": "docker",
                   "config": {
                        "image": "kitematic/hello-world-nginx",
                        "containerConfig": {
                            "exposedPorts": {
                                "8080/tcp": {}
                            }
                        },
                        "hostConfig": {
                        "portBindings": {
                            "80/tcp": [
                                    {"HostPort": "8080/tcp"}
                                ]
                            }
                        }
                    }
                }
            ]
        }
    ]
}