{
    "services": [
        {
            "name": "nginxhello",
            "backend": "docker",
            "repository": "hello-world-nginx",
            "containerConfig": {
                "exposedPorts": {
                    "8080/tcp": {}
                }
            },
            "hostConfig": {
                "portBindings": {
                    "80/tcp": [
                        {"HostPort": "8080/tcp"}
                    ]
                }
            }
        }
    ]
}