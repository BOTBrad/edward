    ],
    "groups": [
        {
            "name": "thirdparty",
            "children": [
                "rabbitmq",
                "haproxy"
            ]
        },
        {
            "name": "stormgrp",
            "children": [
                "thirdparty",
                "admin2",
                "users",
                "storm",
                "locationsstorm",
                "ProfileServer"
            ]
        },
        {
            "name": "pages",
            "children": [
                "stormgrp",
                "sites-staging",
                "sites-storm",
                "sites-cog"
            ]
        },
        {
            "name": "resellers",
            "children": [
                "resellersapi",
                "subscriptions",
                "SalesApiServer"
            ]
        },
        {
            "name": "bag",
            "children": [
                "stormgrp",
                "BeaconServer",
                "dam",
                "bagstorm"
            ]
        },
        {
            "name": "profilesearch",
            "children": [
                "stormgrp",
                "ProfileSearchServer"
            ]
        }
    ]
