        {
            "name": "rabbitmq",
            "path": "$ALPHA",
            "commands": {
                "build": "",
                "launch": "rabbitmq-server",
                "stop": "rabbitmqctl stop"
            },
            "log_properties": {
                "started": "completed"
            },
            "Env": null
        },
        {
