{
    "env": [
        "YEXT_RABBITMQ=localhost"
    ],
    "services": [
        {
            "name": "ProfileServer",
            "path": "$ALPHA",
            "commands": {
                "build": "python tools/icbm/build.py :ProfileServer",
                "launch": "JVM_ARGS='-Xmx3G' build/ProfileServer/ProfileServer",
                "stop": ""
            },
            "log_properties": {
                "started": "Listening"
            },
            "Env": [
                "YEXT_SITE=office"
            ]
        },
        {
            "name": "sites-storm",
            "path": "$ALPHA",
            "commands": {
                "build": "go install yext/pages/sites/sites-storm",
                "launch": "sites-storm",
                "stop": ""
            },
            "log_properties": {
                "started": "Listening"
            },
            "Env": null
        },
        {
            "name": "sites-cog",
            "path": "$ALPHA",
            "commands": {
                "build": "go install yext/pages/sites/sites-cog",
                "launch": "sites-cog",
                "stop": ""
            },
            "log_properties": {
                "started": "Listening"
            },
            "Env": null
        },
        {
            "name": "subscriptions",
            "path": "$ALPHA",
            "commands": {
                "build": "python tools/icbm/build.py :subscriptions_dev",
                "launch": "thirdparty/play/play test src/com/yext/subscriptions",
                "stop": ""
            },
            "log_properties": {
                "started": "Server is up and running"
            },
            "Env": null
        },
        {
            "name": "SalesApiServer",
            "path": "$ALPHA",
            "commands": {
                "build": "python tools/icbm/build.py :SalesApiServer",
