            },
            "Env": [
                "YEXT_SITE=office"
            ]
        },
        {
            "name": "haproxy",
            "path": "$ALPHA",
            "commands": {
                "build": "",
                "launch": "sudo $ALPHA/tools/bin/haproxy_localhost.sh",
                "stop": ""
            },
            "log_properties": {
                "started": "backend"
            },
            "Env": null
        },
        {
            "name": "storm",
            "path": "$ALPHA",
